{
  "name": "ecliptic-earth",
  "type": "module",
  "version": "0.0.1",
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "dev": "astro dev",
    "dev:tina": "tinacms dev -c \"astro dev\"",
    "dev:local": "set \"TINA_PUBLIC_IS_LOCAL=true\" && tinacms dev -c \"astro dev --host\"",
    "dev:mobile": "set \"TINA_PUBLIC_IS_LOCAL=true\" && tinacms dev -c \"astro dev --host 0.0.0.0\"",
    "proxy": "node proxy-server.js",
    "dev:mobile-full": "start /B npm run proxy && npm run dev:mobile",
    "build": "astro build",
    "build:tina": "tinacms build && astro build",
    "build:local": "tinacms build --local --skip-cloud-checks && astro build",
    "build:production": "astro build",
    "sync-images": "node scripts/sync-images.mjs",
    "optimize-single": "node scripts/optimize-single.mjs",
    "auto-detect": "node scripts/auto-detect-sync.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "tina:build": "tinacms build",
    "tina:dev": "tinacms dev",
    "optimize:images": "node scripts/optimize-images.mjs",
    "optimize:batch": "node scripts/optimize-images.mjs",
    "watch:images": "node scripts/watch-images.js",
    "check:optimized": "node scripts/check-optimized-folders.js",
    "sync:tinacloud": "node scripts/sync-tinacloud.js",
    "setup:webhook": "node scripts/setup-webhook.mjs",
    "setup:auto": "npm run setup:webhook",
    "verify:system": "node scripts/verify-system.mjs",
    "test:webhook": "curl -X POST https://kelua.netlify.app/.netlify/functions/auto-optimize-images -H 'Content-Type: application/json' -d '{\"test\": true}'",
    "dev:auto": "start /B npm run watch:images && npm run dev:tina"
  },
  "dependencies": {
    "@tinacms/auth": "^1.0.12",
    "@tinacms/cli": "^1.9.9",
    "@types/uuid": "^10.0.0",
    "astro": "^5.10.0",
    "chokidar": "^4.0.3",
    "form-data": "^4.0.0",
    "http-proxy": "^1.18.1",
    "node-fetch": "^2.7.0",
    "sharp": "^0.34.4",
    "tinacms": "^2.7.9",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.0.0",
    "@rollup/rollup-darwin-x64": "^4.0.0",
    "@rollup/rollup-linux-x64-gnu": "^4.0.0",
    "@rollup/rollup-win32-x64-msvc": "^4.0.0"
  }
}
